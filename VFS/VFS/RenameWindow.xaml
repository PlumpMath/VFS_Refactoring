<Window x:Class="VFS.RenameWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Rename" Height="150" Width="320"
       >
    <Grid>
        <TextBox x:Name="textbox" BorderBrush="Black" BorderThickness="1" Focusable="True" HorizontalAlignment="Stretch" TextWrapping="Wrap" Text="" VerticalAlignment="Center" Height="30" Margin="20,20,9.6,50" FontSize="16"/>
        <Button Content="OK" HorizontalAlignment="Left" Margin="67,72,0,0" VerticalAlignment="Top" Width="75" Click="OK_Click" Height="24" FontFamily="Corbel" FontSize="16" Focusable="True"/>
        <Button Content="Cancel" HorizontalAlignment="Left" Margin="191,72,0,0" VerticalAlignment="Top" Width="75" Click="Cancel_Click" Height="24" FontFamily="Corbel" FontSize="16"/>

    </Grid>
</Window>
